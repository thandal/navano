{
	"name": "Navano",
	"manifest_version": 2,
	"icons": {
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"browser_action": {
		"default_title": "Navano",
		"default_popup": "popup/popup.html"
	},
	"permissions": ["storage", "notifications"],
	"background": {
		"scripts": ["background.js"]
	},
	"externally_connectable": {
		"matches": ["*://total.professional.com/*",
		            "*://host.dais.one/*"]
	}
}
